
<%
grid_id = 0;
notes.each_with_index do |n,i|  
	if n.position.nil? %> 
		<%= render "notes/draggable_postit", postit: n, grid_id: grid_id, top: "-1", left: "-1", z_index: "-1" %>
		<% grid_id += 1 
	else %>
		<%= render "notes/draggable_postit", postit: n, grid_id: "-1", top: n.position.top, left: n.position.left, z_index: n.position.z_index %>
 <% end
end 
%>